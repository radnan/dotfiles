# [Shift +] Tab auto-complete args
TAB: menu-complete
"\e[Z": "\e-1\C-i"

"\C-d": kill-whole-line

# Up/Down search through history
"\e[A": history-search-backward
"\e[B": history-search-forward

# Ctrl + Up/Down to jump lines
"\e[1;5A": beginning-of-line
"\e[1;5B": end-of-line

# Ctrl + Left/Right to jump words
"\e[1;5C": forward-word
"\e[1;5D": backward-word

set completion-ignore-case on
set expand-tilde on

set show-all-if-ambiguous on
set visible-stats on
set -o vi
